{
  "hosting": {
    "public": "dist/ux",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/my-doo-7c70c/us-central1/updateEmail",
        "function": "updateEmail"
      },
      {
        "source": "/my-doo-7c70c/us-central1/pay",
        "function": "pay"
      },
      {
        "source": "/my-doo-7c70c/us-central1/process",
        "function": "process"
      },
      {
        "source": "/my-doo-7c70c/us-central1/initOrder",
        "function": "initOrder"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build"
  }
}
