{
  "hosting": {
    "public": "dist/ux",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers" : [{
        "source": "**/*.@(js|svg|css|woff2|gif|png)",
        "headers": [ {
          "key": "Cache-Control",
          "value": "max-age=7200"
        } ]
      }
    ],
    "rewrites": [
      {
        "source": "/my-doo-7c70c/us-central1/updateEmail",
        "function": "updateEmail"
      },
      {
        "source": "/my-doo-7c70c/us-central1/subscribeEmail",
        "function": "subscribeEmail"
      },
      {
        "source": "/my-doo-7c70c/us-central1/pay",
        "function": "pay"
      },
      {
        "source": "/my-doo-7c70c/us-central1/process",
        "function": "process"
      },
      {
        "source": "/my-doo-7c70c/us-central1/initOrder",
        "function": "initOrder"
      },
      {
        "source": "/my-doo-7c70c/us-central1/sendConfirmationEmail",
        "function": "sendConfirmationEmail"
      },
      {
        "source": "/my-doo-7c70c/us-central1/getCart",
        "function": "getCart"
      },
      {
        "source": "/my-doo-7c70c/us-central1/setStripeCharge",
        "function": "setStripeCharge"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
